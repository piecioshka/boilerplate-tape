    "tap-dot": "^2.0.0",
    "tap-notify": "^1.0.0",
    "tap-nyan": "^1.1.0",
    "tap-prettify": "^0.0.2",
    "tap-spec": "^5.0.0",
    "tape": "^5.3.2"
  }
